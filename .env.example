# AWS 설정
# ECR 접근을 위한 AWS 자격증명
AWS_ACCESS_KEY_ID=your-access-key-id
AWS_SECRET_ACCESS_KEY=your-secret-access-key
AWS_REGION=us-east-1

# ECR 레지스트리 주소
AWS_ECR_REGISTRY=242552818991.dkr.ecr.us-east-1.amazonaws.com

# Kubernetes 설정
# 배포할 네임스페이스
K8S_NAMESPACE=default

# ImagePullSecret 이름
K8S_IMAGE_PULL_SECRET=ecr-secret

# ImagePullSecret 자동 생성 여부 (true/false)
K8S_AUTO_CREATE_SECRET=true